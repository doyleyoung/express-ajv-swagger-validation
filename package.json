{
  "name": "express-ajv-swagger-validation",
  "version": "0.10.2",
  "description": "Input validation using Swagger (Open API) and ajv",
  "main": "src/middleware.js",
  "nyc": {
    "report-dir": "./coverage",
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 90,
    "include": [
      "src/*.js"
    ],
    "exclude": [
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "test": "nyc node_modules/mocha/bin/_mocha --recursive ./test/*/*/*-test.js ./test/*/*-test.js ./test/*-test.js",
    "node6-test": "nyc node_modules/mocha/bin/_mocha --recursive ./test/express/*/*-test.js ./test/express/*-test.js ./test/*-test.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "doctoc": "npm install -g doctoc && doctoc README.md"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Zooz/express-ajv-swagger-validation.git"
  },
  "keywords": [
    "ajv",
    "express",
    "swagger",
    "OpenAPI",
    "open api",
    "input validation",
    "validation",
    "validator",
    "koa"
  ],
  "bugs": {
    "url": "https://github.com/Zooz/express-ajv-swagger-validation"
  },
  "homepage": "https://github.com/Zooz/express-ajv-swagger-validation",
  "author": "Idan Tovi",
  "license": "Apache-2.0",
  "dependencies": {
    "api-schema-builder": "^1.0.9"
  },
  "devDependencies": {
    "ajv-keywords": "^3.4.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-sinon": "^2.8.1",
    "clear-require": "^2.0.0",
    "coveralls": "^3.0.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.16.4",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.4.0",
    "mocha": "^4.1.0",
    "multer": "^1.4.1",
    "nyc": "^13.3.0",
    "request": "^2.88.0",
    "rewire": "^2.5.2",
    "sinon": "^4.5.0",
    "supertest": "^3.4.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
